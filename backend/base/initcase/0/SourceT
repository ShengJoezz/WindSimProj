FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "1";
    object      SourceT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    back
    {
        type            symmetry;//原始为symmetryPlane joe修改
    }
    front
    {
        type            symmetry;//原始为symmetryPlane joe修改
    }
    bot
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    top
    {
        type            symmetry;//原始为symmetryPlane joe修改
    }
}


